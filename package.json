{
  "name": "sipdemo",
  "version": "1.0.1",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "electron src/index.js",
    "build": "electron-builder",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "electron-updater": "^4.3.5"
  },
  "devDependencies": {
    "electron": "^11.2.1",
    "electron-builder": "^22.10.4",
    "electron-packager": "^15.2.0",
    "typescript": "^4.1.3"
  },
  "build": {
    "appId": "id.dotspace.sipdemo",
    "asar": true,
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "category": "public.app-category.development",
      "darkModeSupport": true
    },
    "dmg": {
      "iconSize": 160,
      "contents": [
        {
          "x": 180,
          "y": 170
        },
        {
          "x": 480,
          "y": 170,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Development"
    },
    "win": {
      "target": "nsis"
    },
    "publish": [
      {
        "provider": "generic",
        "url": "http://localhost:5000"
      }
    ]
  }
}
